{
    "version": 2,
    "builds": [
      {
        "src": "client/**",
        "use": "@vercel/static"
      },
      {
        "src": "server/src/api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/src/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  