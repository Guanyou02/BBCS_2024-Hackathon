{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "dest": "/"
      },
      {
        "src": "server/api/*.js",
        "use": "@vercel/node",
        "dest": "/api"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  